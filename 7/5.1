var SENTINEL = 0;

// Prints whether the entered number is even or odd
function start(){
	while(true) {
	    var n = readInt("");
	    if(n == SENTINEL) {
	        println("Done!");
	        break;
	    }
	    var b = isEven(n);
	    if(b == true) {
	        println("Even");
	    } else {
	        println("Odd");
	    }
	    
	}
}
function isEven(i) {
    if(i % 2 == 0) {
        return true;
    } else {
        return false;
    }
}
