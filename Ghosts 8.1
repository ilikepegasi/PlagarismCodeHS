function letterGrade(score) {
    var g;
    switch(Math.floor(score / 10)) {
        case 10:
            return "A+";
        case 9: 
            g = "A";
            break;
        case 8:
            g = "B";
            break;
        case 7:
            g = "C";
            break;
        case 6:
            g = "D";
            break;
        default:
            return "F";
    }
    if(score % 10 <= 2) {
        g = g+"-";
    } else {
        g = g+"+";
    }
    return g;
}
